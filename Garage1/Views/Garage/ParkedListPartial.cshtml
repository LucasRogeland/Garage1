@model IEnumerable<Models.ParkingDetails>

<tr>
    <th>
        Name
    </th>
    <th>
        @Html.DisplayNameFor(model => model.Vehicle.VehicleType)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.Vehicle.License)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.ParkingSpaceId)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.CheckInTime)
    </th>
    <th></th>
</tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Member.LastName), @Html.DisplayFor(modelItem => item.Member.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.VehicleType.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.License)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkingSpaceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckInTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}
