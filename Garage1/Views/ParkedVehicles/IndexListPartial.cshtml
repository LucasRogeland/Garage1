@model Garage1.ViewModel.IndexListPartialViewModel

<tr>
    <th class="clickable @if (Model.Target == "Licens") { @Model.CssClassDesc } " onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().Vehicle.Licens)
    </th>
    <th class="clickable @if (Model.Target == "VehicleType") { @Model.CssClassDesc }" onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().Vehicle.VehicleType)
    </th>
    <th class="clickable @if (Model.Target == "Manufacturer") { @Model.CssClassDesc }" onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().Vehicle.Manufacturer)
    </th>
    <th class="clickable @if (Model.Target == "Model") { @Model.CssClassDesc }" onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().Vehicle.Model)
    </th>
    <th class="clickable @if (Model.Target == "Color") { @Model.CssClassDesc }" onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().Vehicle.Color)
    </th>
    <th class="clickable @if (Model.Target == "CheckInTime") { @Model.CssClassDesc }" onclick="sort(this)">
        @Html.DisplayNameFor(model => model.Details.FirstOrDefault().CheckInTime)
    </th>
    <th></th>
</tr>


@foreach (var item in Model.Details)
{

    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.Licens)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.VehicleType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.Manufacturer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CheckInTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "btn btn-success" })
            @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Check Out", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}
